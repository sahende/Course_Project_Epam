{
  "name": "course-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest --coverage",
    "dev": "npm run dev:server",
    "test:unit": "jest --testPathPattern=tests/unit --runInBand",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:contract": "jest --testPathPattern=tests/contract",
    "test:e2e": "npx playwright test",
    "coverage": "jest --coverage --coverageDirectory=coverage",
    "dev:server": "npx tsc -p tsconfig.build.json && node ./scripts/dev-server.js",
    "mutate": "npx stryker run"
  },
  "dependencies": {
    "@prisma/client": "^4.16.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.0",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^13.0.0",
    "msw": "^0.49.2",
    "nodemon": "^3.1.14",
    "playwright": "^1.40.0",
    "prisma": "^4.16.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint -- --fix",
      "git add"
    ]
  }
}
